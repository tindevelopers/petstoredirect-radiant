{
  "name": "frontend-design-system",
  "version": "2.0.0",
  "description": "A comprehensive design system for building modern frontend websites. Includes components from Catalyst UI Kit and TailAdmin Pro.",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "styles",
    "vendors",
    "README.md",
    "CHANGELOG.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./components": {
      "import": "./dist/components/index.esm.js",
      "require": "./dist/components/index.js",
      "types": "./dist/components/index.d.ts"
    },
    "./tokens": {
      "import": "./dist/tokens/index.esm.js",
      "require": "./dist/tokens/index.js",
      "types": "./dist/tokens/index.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.esm.js",
      "require": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils/index.esm.js",
      "require": "./dist/utils/index.js",
      "types": "./dist/utils/index.d.ts"
    },
    "./styles/globals.css": "./styles/globals.css",
    "./styles/tailwind.config": {
      "import": "./styles/tailwind.config.js",
      "require": "./styles/tailwind.config.js"
    },
    "./vendors/catalyst": "./vendors/catalyst-ui-kit/typescript",
    "./vendors/tailadmin": "./vendors/tailadmin-pro-ts/src/components"
  },
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "lint:check": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "release": "changeset publish",
    "version": "changeset version",
    "turbo:build": "turbo build",
    "turbo:lint": "turbo lint",
    "turbo:test": "turbo test",
    "turbo:type-check": "turbo type-check",
    "turbo:dev": "turbo dev"
  },
  "keywords": [
    "design-system",
    "react",
    "typescript",
    "tailwindcss",
    "frontend",
    "website",
    "ui-components",
    "design-tokens",
    "catalyst",
    "tailadmin",
    "nextjs",
    "landing-page",
    "marketing-site"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/frontend-design-system.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/frontend-design-system/issues"
  },
  "homepage": "https://github.com/your-username/frontend-design-system#readme",
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "tailwindcss": ">=4.0.0"
  },
  "dependencies": {
    "@headlessui/react": "^2.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "framer-motion": "^11.0.0",
    "motion": "^10.18.0",
    "tailwind-merge": "^2.2.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.0",
    "prettier": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.0",
    "rollup": "^4.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "tailwindcss": "^4.0.0",
    "ts-jest": "^29.1.0",
    "tslib": "^2.6.0",
    "turbo": "^2.5.8",
    "typescript": "^5.2.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
