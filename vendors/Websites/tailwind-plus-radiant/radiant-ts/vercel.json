{
  "buildCommand": "npx turbo build",
  "devCommand": "npx turbo dev",
  "installCommand": "npm ci",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "functions": {
    "app/**/*.tsx": {
      "runtime": "nodejs20.x"
    }
  },
  "env": {
    "NEXT_PUBLIC_VERCEL_URL": "@next_public_vercel_url"
  }
}
